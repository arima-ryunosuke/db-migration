{
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "name": "ryunosuke/db-migration",
  "type": "library",
  "description": "Database Migration Commands",
  "keywords": [
    "ryunosuke",
    "library",
    "migration",
    "database",
    "doctrine"
  ],
  "homepage": "https://github.com/arima-ryunosuke/db-migration",
  "license": "MIT",
  "authors": [
    {
      "name": "ryunosuke",
      "email": "ryunosuke.arima@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ryunosuke\\DbMigration\\": "src/DbMigration/"
    },
    "files": [
      "src/functions.stub"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ryunosuke\\Test\\DbMigration\\": "tests/DbMigration"
    },
    "files": [
      "tests/functions.php"
    ]
  },
  "bin": [
    "bin/dbmigration"
  ],
  "repositories": [
    {
      "url": "https://github.com/arima-ryunosuke/dbal.git",
      "type": "vcs"
    }
  ],
  "require": {
    "php": ">=7.4",
    "doctrine/dbal": "3.10.*",
    "symfony/console": "5.*",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "ryunosuke/functions": "1.6.*",
    "phpunit/phpunit": "9.*"
  },
  "scripts": {
    "build": [
      "@export",
      "@test",
      "@pharize"
    ],
    "export": [
      "export-function",
      "exclude-internal"
    ],
    "test": [
      "vendor/bin/phpunit --configuration=tests/phpunit.xml"
    ],
    "pharize": [
      "bin/box.phar compile -c box.json"
    ],
    "post-install-cmd": [
      "@export"
    ],
    "post-update-cmd": [
      "@export"
    ]
  }
}
